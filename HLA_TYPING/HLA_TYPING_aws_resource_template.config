[Docker]
    [[hlala]]
        image = davelabhub/hla-la:latest
        [[[hlala]]]
            resource_type   = hla-la
            path            = HLA-LA.pl

[Path]
    [[reference_genome]]
	resource_type = ref
	path = s3://davelab-data/GRCh38.p12.genome.plus.ERCC.HLA/GRCh38.p12.genome.plus.ERCC.HLA.fa
    [[graph]]
	resource_type = graph
	path = s3://davelab-data/PRG_MHC_GRCh38_withIMGT/
    [[additional_ref]]
        resource_type = additional_ref
        path = s3://davelab-data/ERCC_HLA.txt
